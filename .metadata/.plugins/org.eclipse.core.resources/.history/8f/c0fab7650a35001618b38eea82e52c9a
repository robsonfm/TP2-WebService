package org.engsoft.webservice;

public class Livro {
	
	private int isbn;
	private String nome;
	private String autor;
	private String editora;
	private int ano;
	
	public Livro(int isbn, String nome, String autor, String editora, int ano) {
		this.isbn = isbn;
		this.nome = nome;
		this.autor = autor;
		this.editora = editora;
		this.ano = ano;
	}

	public int getIsbn() {
		return isbn;
	}

	public String getNome() {
		return nome;
	}

	public String getAutor() {
		return autor;
	}

	public String getEditora() {
		return editora;
	}

	public int getAno() {
		return ano;
	}

	
			
}
